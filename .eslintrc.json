{
  "env": {
    "browser": true,
    "node": true
  },
  "extends": ["airbnb-base", "prettier", "eslint-config-prettier", "esnext"],
  "parserOptions": {
    "ecmaVersion": 11,
    "sourceType": "module"
  },
  "rules": {
    "prettier/prettier": [
      "error",
      {
        "trailingComma": "es5",
        "singleQuote": true,
        "printWidth": 100
      }
    ]
  },
  "plugins": ["prettier"]
}
